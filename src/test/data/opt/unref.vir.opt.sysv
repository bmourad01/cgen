module unref

function $sum(l %s/rdi) {
  %3 = slot 8, align 8
@2:
  st.l %s, %3 ; @22
  %a.1 = ld.w %3 ; @4
  %0 = add.l %3, 0x4_l ; @19
  %b.1 = ld.w %0 ; @6
  %1 = add.w %a.1, %b.1 ; @20
  ret rax/%1
}

export function $sump(l %p/rdi) {
@8:
  %6.1 = ld.l %p ; @25
  %sum.1/w/rax = call $sum(%6.1/rdi) ; @10
  ret rax/%sum.1
}

export function $mkt(w %a/rdi, w %b/rsi) {
  %8 = slot 8, align 8
@11:
  st.w %a, %8 ; @12
  %2 = add.l %8, 0x4_l ; @21
  st.w %b, %2 ; @14
  %9 = ld.l %8 ; @28
  st.l %9, %8 ; @29
  ret rax/%9
}

export function $sumt(w %a/rdi, w %b/rsi) {
@16:
  %12.1/l/rax = call $mkt(%a/rdi, %b/rsi) ; @17
  %sum.1/w/rax = call $sum(%12.1/rdi) ; @18
  ret rax/%sum.1
}
