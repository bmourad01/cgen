module sumphi

function $sum(l %s/rdi) {
  %p = slot 8, align 8
@2:
  st.l %s, %p ; @22
  %a.1 = ld.w %p ; @4
  %0 = add.l %p, 0x4_l ; @19
  %b.1 = ld.w %0 ; @6
  %1 = add.w %a.1, %b.1 ; @20
  ret rax/%1
}

export function $sumphi(l %a/rdi, l %b/rsi, w %x/rdx) {
@8:
  %2.1 = slt.w %x, 0x0_w ; @21
  br %2.1, @9, @10
@9:
  jmp @14(%a)
@10:
  jmp @14(%b)
@14(%rb.3):
  %s.1/w/rax = call $sum(%rb.3/rdi) ; @18
  ret rax/%s.1
}
