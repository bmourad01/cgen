module sumphi

function $sum(l %s/rdi) {
  %3 = slot 8, align 8
@2:
  st.l %s, %3 ; @22
  %a.1 = ld.w %3 ; @4
  %0 = add.l %3, 0x4_l ; @19
  %b.1 = ld.w %0 ; @6
  %1 = add.w %a.1, %b.1 ; @20
  ret rax/%1
}

export function $sumphi(l %a/rdi, l %b/rsi, w %x/rdx) {
  %ra = slot 8, align 8
@8:
  st.l %a, %ra ; @28
  %2.1 = slt.w %x, 0x0_w ; @21
  br %2.1, @9, @10
@9:
  jmp @14(%a)
@10:
  jmp @14(%b)
@14(%6.3):
  st.w 0x5_w, %ra ; @17
  %s.1/w/rax = call $sum(%6.3/rdi) ; @18
  ret rax/%s.1
}
