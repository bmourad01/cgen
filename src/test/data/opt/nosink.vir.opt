module nosink

export function w $foo(w %x) {
@2:
  %0 = lsl.w %x, 0x2_w ; @16
  %1 = add.w %0, %x ; @17
  %2 = eq.w %x, 0x0_w ; @18
  br %2, @3, @4
@3:
  %4 = add.w %1, 0x1_w ; @20
  jmp @7(%4)
@4:
  %6 = eq.w %x, 0x1_w ; @22
  br %6, @9, @10
@9:
  %7 = add.w %1, 0x5_w ; @23
  jmp @7(%7)
@10:
  %8 = add.w %1, 0x6_w ; @24
  jmp @7(%8)
@7(%a.2):
  %3 = sub.w %a.2, 0x1_w ; @19
  ret %3
}
