module switch1

export function w $foo(w %x, w %y) {
@28:
  %acc = copy.w 0x0_w ; @29
  switch.w %x, @5 [0x0_w -> @26,
                   0x1_w -> @24,
                   0x2_w -> @21,
                   0x3_w -> @17,
                   0x4_w -> @8]
@26:
  %acc = add.w %y, 0x1_w ; @27
