module coalesce1

export function w $f(w %x, w %y, w %z) {
  %a = slot 8, align 8
  %b = slot 8, align 8
@2:
  st.w %x, %a ; @4
  jmp @3
@3:
  %u1.1 = ld.w %a ; @6
  jmp @5
@5:
  jmp @7
@7:
  st.w %y, %b ; @9
  jmp @8
@8:
  %u2.1 = ld.w %b ; @11
  jmp @10
@10:
  st.w %z, %a ; @13
  jmp @12
@12:
  %u3.1 = ld.w %a ; @15
  jmp @14
@14:
  %u4.1 = ld.w %b ; @17
  jmp @16
@16:
  %r.1 = add.w %u1.1, %u2.1 ; @18
  %r.2 = add.w %r.1, %u3.1 ; @19
  %r.3 = add.w %r.2, %u4.1 ; @20
  ret %r.3
}
