module evenodd

function $abs(w %n/rdi) {
@2:
  %4 = slt.w %n, 0x0_w ; @25
  %5 = neg.w %n ; @26
  %6 = sel.w %4, %5, %n ; @27
  ret rax/%6
}

export function $even(w %n/rdi) {
@7:
  %n.1/w/rax = call $abs(%n/rdi) ; @10
  %7 = eq.w %n.1, 0x0_w ; @28
  br %7, @23(0x1_b), @9
@9:
  %8 = sub.w %n.1, 0x1_w ; @29
  %b.1/b/rax = call $odd(%8/rdi) ; @13
  jmp @23(%b.1)
@23(%2):
  ret rax/%2
}

export function $odd(w %n/rdi) {
@14:
  %n.1/w/rax = call $abs(%n/rdi) ; @17
  %9 = eq.w %n.1, 0x0_w ; @30
  br %9, @24(0x0_b), @16
@16:
  %10 = sub.w %n.1, 0x1_w ; @31
  %b.1/b/rax = call $even(%10/rdi) ; @20
  jmp @24(%b.1)
@24(%3):
  ret rax/%3
}
