  br %18, @20, @21
@20:
  %19 = add.w %x, 0x5_w ; @115
  %20 = lsl.w %19, 0x4_w ; @116
  %21 = lsl.w %19, 0x1_w ; @117
  %22 = sub.w %20, %21 ; @118
  %23 = sub.w %22, %19 ; @119
