module irreducible1

export function w $foo(w %x) {
start:
  %0 = and.w %x, 0x1_w;
  %1 = eq.w %0, 0x0_w;
  %i.2 = copy.w 0x0_w;
  loop {
    if slt.w %i.2, %x {
      %i.6 = copy.w %i.2;
      @5:
        %5 = add.w %i.6, 0x1_w;
        if eq.w %5, %x {
          %i.3 = copy.w %5
        } else {
          %i.4 = copy.w %5;
          %3 = sub.w %i.4, 0x1_w;
          if eq.w %3, 0x0_w {
            %i.3 = copy.w %3
          } else {
            %i.6 = copy.w %3;
            goto @5
          }
        }
    } else {
      %i.4 = copy.w %i.2;
      %3 = sub.w %i.4, 0x1_w;
      if eq.w %3, 0x0_w {
        %i.3 = copy.w %3
      } else {
        %i.6 = copy.w %3;
        goto @5
      }
    };
    if %1 {
      break
    } else {
      %i.2 = copy.w %i.3
    }
  };
  ret %i.3
}
